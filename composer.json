{
    "name": "loevgaard/dandomain-consignment-bundle",
    "description": "Symfony bundle to handle consignment manufacturers in Dandomain",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^1.6",
        "loevgaard/dandomain-consignment-entities": "dev-master",
        "loevgaard/dandomain-foundation-bundle": "dev-master",
        "loevgaard/dandomain-foundation-entities": "dev-master",
        "psr/log": "^1.0",
        "symfony/framework-bundle": "^3.3||^4.0",
        "symfony/options-resolver": "^3.0||^4.0",
        "symfony/yaml": "^3.0||^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^6.5",
        "symfony/phpunit-bridge": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Loevgaard\\DandomainConsignmentBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "./vendor/bin/php-cs-fixer fix --verbose --dry-run",
        "fix-style": "./vendor/bin/php-cs-fixer fix --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
